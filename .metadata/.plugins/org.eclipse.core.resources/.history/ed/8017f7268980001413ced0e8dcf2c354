package methodpractice;
import java.util.*;
import villa7.Print;

public class MethodPractice {
	private Print p = new Print();
	
	public static void main(String[] args) {
		powersOfTwo();
	}
	
	private static void powersOfTwo() {
		int a = 2;
		for (int i = 0; i < 10; i++) {
			p.l(Math.pow(a, i) + " ");
		}
	}

}
